{
  "name": "@tevm/shadow",
  "version": "0.1.0",
  "description": "TypeScript bindings for Shadow Solidity parser",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "wasm"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "biome check --write .",
    "format": "biome format --write .",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solidity",
    "parser",
    "wasm",
    "typescript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.2.6",
    "@types/bun": "^1.3.0",
    "typescript": "^5.9.3"
  }
}
